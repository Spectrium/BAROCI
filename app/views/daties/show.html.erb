<div class="container pt-5 pb-5 text-center">
	<h1>
		Voici la date du mandat
	</h1>
	<div class="">
		<p>
			Debut : <%= @date.start_date %><br>
			Fin : <%= @date.end_date %><br><br>
		</p>
	</div>
	<div class="d-flex justify-content-center">
		<% if user_signed_in? %>
			<% if current_user.is_admins == true %>
				<%= link_to "Editer la date du Mandat", edit_daty_path(@date.id),class:'btn btn-success mr-3' %><br>
				<%= link_to "Effacer la date du Mandat", daty_path(@date.id), method:"delete",class:'btn btn-danger' %>
			<% end %>
		<% end %>
	</div>
</div>