<div class="container">
	<div class="row">
		<p>
			Debut : <%= @date.start_date %><br>
			Fin : <%= @date.end_date %><br><br>
		</p>
	</div>
	<% if user_signed_in? %>
		<% if current_user.is_admins == true %>
			<%= link_to "Editer la date du Mandat", edit_daty_path(@date.id) %><br><br>
			<%= link_to "Effacer la date du Mandat", daty_path(@date.id), method:"delete" %>
		<% end %>
	<% end %>
</div>